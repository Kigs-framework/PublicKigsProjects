<?xml version="1.0" encoding="iso-8859-1"?>
<Inst N="thmbnail" T="UIImage">
	<Attr N="Priority" V="10"/>
	<Attr N="Dock" V="{0.5,0.5}"/>
	<Attr N="PreScale" V="{0.6,0.6}"/>
	<Attr N="Anchor" V="{0.5,0.5}"/>
	<Inst N="ChannelName" T="UIText">
		<Attr N="Priority" V="12"/>
		<Attr N="Text" V="channelName"/>
		<Attr N="Dock" V="{0.5,1.0}"/>
		<Attr N="Anchor" V="{0.5,0.7}"/>
		<Attr N="Size" V="{-1,-1}"/>
		<Attr N="Font" V="Calibri.ttf"/>
		<Attr N="FontSize" V="18"/>
		<Attr N="MaxWidth" V="200"/>		
		<Attr N="Color" V="{0.1, 0.2, 0.2, 1.0}"/>
		<Inst N="txtBackground" T="UIImage">
				<Attr N="TextureName" V="FondTexte.png"/>
				<Attr N="SizeModeX" V="Add"/>
				<Attr N="SizeModeY" V="Add"/>
				<Attr N="Size" V="{4,4}"/>
				<Attr N="Sliced" V="{6.0,6.0}"/>
				<Attr N="Priority" V="11"/>
				<Attr N="Anchor" V="{0.500000,0.500000}"/>
				<Attr N="Dock" V="{0.500000,0.500000}"/>
		</Inst>
	</Inst>
	<Inst N="HasRetweet" T="UIText">
			<Attr N="Priority" V="12"/>
			<Attr N="Text" V="Percent"/>
			<Attr N="Dock" V="{1.0,0.0}"/>
			<Attr N="Anchor" V="{0.4,0.5}"/>
			<Attr N="Size" V="{-1,-1}"/>
			<Attr N="Font" V="Calibri.ttf"/>
			<Attr N="FontSize" V="18"/>
			<Attr N="MaxWidth" V="200"/>	
			<Attr N="Color" V="{0.1, 0.2, 0.2, 1.0}"/>		
		<Inst N="hasRTBackground"  T="UIImage">
				<Attr N="TextureName" V="FondTexte.png"/>
				<Attr N="SizeModeX" V="Add"/>
				<Attr N="SizeModeY" V="Add"/>
				<Attr N="Size" V="{4,4}"/>
				<Attr N="Sliced" V="{6.0,6.0}"/>
				<Attr N="Priority" V="11"/>
				<Attr N="Anchor" V="{0.500000,0.500000}"/>
				<Attr N="Dock" V="{0.500000,0.500000}"/>
		</Inst>
	</Inst>
	<Inst N="TweetCount" T="UIText">
			<Attr N="Priority" V="12"/>
			<Attr N="Text" V="Percent"/>
			<Attr N="Dock" V="{0.0,0.0}"/>
			<Attr N="Anchor" V="{0.6,0.5}"/>
			<Attr N="Size" V="{-1,-1}"/>
			<Attr N="Font" V="Calibri.ttf"/>
			<Attr N="FontSize" V="18"/>
			<Attr N="MaxWidth" V="200"/>	
			<Attr N="Color" V="{0.1, 0.2, 0.2, 1.0}"/>		
		<Inst N="tweetBackground"  T="UIImage">
				<Attr N="TextureName" V="FondTexte.png"/>
				<Attr N="SizeModeX" V="Add"/>
				<Attr N="SizeModeY" V="Add"/>
				<Attr N="Size" V="{4,4}"/>
				<Attr N="Sliced" V="{6.0,6.0}"/>
				<Attr N="Priority" V="11"/>
				<Attr N="Anchor" V="{0.500000,0.500000}"/>
				<Attr N="Dock" V="{0.500000,0.500000}"/>
		</Inst>
	</Inst>
	<Inst N="shape" T="UIShapeDisc">
		<Attr N="SliceCount" V="32"/>
	</Inst>
	
	<Inst N="thumbbackground" T="UIPanel">
		<Attr N="SizeModeX" V="Add"/>
		<Attr N="SizeModeY" V="Add"/>
		<Attr N="Size" V="{8,8}"/>
		<Attr N="Priority" V="8"/>
		<Attr N="Anchor" V="{0.500000,0.500000}"/>
		<Attr N="Dock" V="{0.500000,0.500000}"/>
		<Attr N="Color" V="{0.0, 0.0, 0.0}"/>
		<Inst N="shape" T="UIShapeDisc">
			<Attr N="SliceCount" V="32"/>
		</Inst>
	</Inst>
</Inst>